{
   "presets": [
      [
         "next/babel",
         {
            "preset-react": {
               "runtime": "automatic",
               "importSource": "@emotion/react"
            }
         }
      ]
   ],
   "plugins": [
      "@emotion/babel-plugin",
      [
         "formatjs",
         {
            "ast": true,
            "idInterpolationPattern": "[sha512:contenthash:base64:6]",
            "extractFromFormatMessageCall": true
         }
      ]
   ]
}
