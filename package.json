{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next --openssl-legacy-provider",
    "build": "yarn lang:extract && yarn lang:compile && next build",
    "start": "next start",
    "type-check": "tsc",
    "format": "prettier --write **/*.{tsx,ts,scss}",
    "lint": "eslint --fix **/*.{tsx,ts}",
    "lang:extract": "formatjs extract {pages,components}/**/*.{ts,tsx} --format simple --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file lang/en.json",
    "lang:compile": "formatjs compile-folder --ast --format simple lang compiled-lang"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@formatjs/cli": "^4.2.21",
    "@formatjs/intl-pluralrules": "^4.0.27",
    "@styled-icons/fa-regular": "^10.26.0",
    "@styled-icons/fa-solid": "^10.32.0",
    "@types/formidable": "^1.2.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.5",
    "@types/mongodb": "^3.6.12",
    "@types/next-auth": "^3.15.0",
    "babel-plugin-react-intl": "^8.2.25",
    "cuid": "^2.1.8",
    "formidable": "^1.2.2",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.13",
    "next": "latest",
    "next-auth": "^3.27.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-intl": "^5.20.3",
    "react-tooltip": "^4.2.17",
    "slugify": "^1.5.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{tsx,ts,scss}": "prettier --write",
    "**/*.{tsx,ts}": "eslint --fix"
  }
}
